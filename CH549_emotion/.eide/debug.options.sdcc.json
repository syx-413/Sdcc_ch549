{
    "version": 3,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "device": "mcs51",
        "optimize-type": "size",
        "stack-auto": true,
        "use-non-free": true,
        "misc-controls": "--xram-size 0x0800 --code-size 0xF000"
    },
    "c/cpp-compiler": {
        "language-c": "sdcc2x"
    },
    "asm-compiler": {},
    "linker": {
        "$mainFileName": "main",
        "output-format": "hex"
    }
}